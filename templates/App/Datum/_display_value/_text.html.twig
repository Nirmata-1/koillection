<span data-value="{{ datum.value }}" data-type="string"></span>

{% if underlinedTags is defined %}
    {% for value in datum.value|split(',') %}
        {{ underlinedTags[value|trim] is defined ? underlinedTags[value|trim]|raw : value }}{{ not loop.last ? ',' }}
    {% endfor %}
{% else %}
    {{ datum.value }}
{% endif %}